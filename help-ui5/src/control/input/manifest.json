{
	"_version": "1.12.0",
	"sap.app": {
		"id": "help",
		"type": "application",
		"i18n": "i18n/i18n.properties",
		"title": "Help Application",
		"description": "UI5 Example",
		"applicationVersion": {
			"version": "1.0.0"
		}
	},
	"sap.ui": {
		"technology": "UI5",
		"deviceTypes": {
			"desktop": true,
			"tablet": true,
			"phone": true
		}
	},
	
	"sap.ui5": {
		"rootView": {
			"viewName": "help.view.App",
			"type": "XML",
			"async": true,
			"id": "app"
		},
		"dependencies": {
			"minUI5Version": "1.60",
			"libs": {
				"sap.m": {}
			}
		},
		"models": {
			"i18n": {
				"type": "sap.ui.model.resource.ResourceModel",
				"settings": {
					"bundleName": "help.i18n.i18n"
				}
			}
		}
	},
	"config": {
		"help": {
			"stretch": true
		}
	}
}
