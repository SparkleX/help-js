<mvc:View
		xmlns:mvc="sap.ui.core.mvc"
		xmlns:table="sap.ui.table"
		xmlns="sap.m"
		width="100%">
			<table:Table rows="{/JDT1}">
				<table:extension>
					<OverflowToolbar id="otb1">
						<Button icon="sap-icon://add" type="Transparent"/>
						<Button icon="sap-icon://delete" type="Transparent"/>
						<Button icon="sap-icon://action-settings" type="Transparent"/>

					</OverflowToolbar>
				</table:extension>
				<table:columns>
					<table:Column width="12em">
						<Label text="Account" />
						<table:template>
							<Text text="{AcctCode}" />
						</table:template>
					</table:Column>
					<table:Column width="12em">
						<Label text="Credit" />
						<table:template>
							<Text text="{Credit}" />
						</table:template>
					</table:Column>
					<table:Column width="12em">
						<Label text="Debit" />
						<table:template>
							<Text text="{Debit}" />
						</table:template>
					</table:Column>

				</table:columns>
			</table:Table>		
</mvc:View>
